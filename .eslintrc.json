{

	"ecmaFeatures": {
		"binaryLiterals": false,
		"blockBindings": false,
		"defaultParams": false,
		"forOf": false,
		"generators": false,
		"objectLiteralComputedProperties": false,
		"objectLiteralDuplicateProperties": false,
		"objectLiteralShorthandMethods": false,
		"objectLiteralShorthandProperties": false,
		"octalLiterals": false,
		"regexUFlag": false,
		"regexYFlag": false,
		"templateStrings": false,
		"unicodeCodePointEscapes": false,
		"jsx": false
	},

	"env": {
		"browser": true,
		"node": false,
		"amd": false,
		"mocha": false,
		"jasmine": false,
		"phantomjs": false,
		"jquery": true,
		"prototypejs": false,
		"shelljs": false
	},

	"globals": {
		"sap": true,
		"com": true
	},

	"plugins": [
	],

	"rules": {
		"comma-dangle": [2, "never"],
		"no-cond-assign": 2,
		"no-console": 0,
		"no-constant-condition": 2,
		"no-control-regex": 1,
		"no-debugger": 1,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-empty": 0,
		"no-empty-class": 1,
		"no-ex-assign": 1,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": 0,
		"no-extra-semi": 1,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-reserved-keys": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 0,
		"valid-typeof": 1,



		"block-scoped-var": 0,
		"complexity": 0,
		"consistent-return": 2,
		"curly": 2,
		"default-case": 0,
		"dot-notation": 0,
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-alert": 1,
		"no-caller": 2,
		"no-div-regex": 1,
		"no-else-return": 0,
		"no-empty-label": 1,
		"no-eq-null": 0,
		"no-eval": 2,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-fallthrough": 1,
		"no-floating-decimal": 1,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 1,
		"no-lone-blocks": 1,
		"no-loop-func": 1,
		"no-multi-spaces": 1,
		"no-multi-str": 0,
		"no-native-reassign": 2,
		"no-new": 1,
		"no-new-func": 1,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-octal-escape": 1,
		"no-process-env": 1,
		"no-proto": 2,
		"no-redeclare": 1,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-self-compare": 1,
		"no-sequences": 1,
		"no-unused-expressions": 1,
		"no-void": 1,
		"no-warning-comments": 0,
		"no-with": 2,
		"radix": 1,
		"vars-on-top": 0,
		"wrap-iife": [1, "any"],
		"yoda": 0,



		"global-strict": 0,
		"no-extra-strict": 0,
		"strict": 0,



		"no-catch-shadow": 0,
		"no-delete-var": 2,
		"no-label-var": 1,
		"no-shadow": 1,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 0,
		"no-undefined": 1,
		"no-unused-vars": 2,
		"no-use-before-define": 2,



		"handle-callback-err": 0,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 0,
		"no-process-exit": 0,
		"no-restricted-modules": 0,
		"no-sync": 0,



		"brace-style": [2, "1tbs", {
			"allowSingleLine": false
		}],
		"camelcase": [2, {
			"properties": "always"
		}],
		"comma-spacing": [2, {
			"before": false,
			"after": true
		}],
		"comma-style": [2, "last"],
		"consistent-this": [2, "that"],
		"eol-last": 0,
		"func-names": 0,
		"func-style": 0,
		"key-spacing": 0,
		"max-nested-callbacks": [1, 3],
		"new-cap": 2,
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-inline-comments": 2,
		"no-lonely-if": 1,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": 1,
		"no-nested-ternary": 2,
		"no-new-object": 2,
		"no-space-before-semi": 2,
		"no-spaced-func": 2,
		"no-ternary": 0,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 0,
		"no-wrap-func": 1,
		"one-var": 0,
		"operator-assignment": 0,
		"padded-blocks": [2, "never"],
		"quote-props": 0,
		"quotes": [1, "single"],
		"semi": [2, "always"],
		"sort-vars": 0,
		"space-after-function-name": 0,
		"space-after-keywords": [1, "never"],
		"space-before-blocks": [1, "never"],
		"space-before-function-paren": [2, "never"],
		"space-in-brackets": [1, "never"],
		"space-in-parens": [1, "never"],
		"space-infix-ops": [1],
		"space-return-throw-case": 2,
		"space-unary-ops": [1, {"words": true, "nonwords": false}],
		"spaced-line-comment": 0,
		"wrap-regex": 0,


		"no-var": 0,
		"generator-star": 0,


		"max-depth": 0,
		"max-len": 0,
		"max-params": 0,
		"max-statements": 0,
		"no-bitwise": 1,
		"no-plusplus": 0
	}
}